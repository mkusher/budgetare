{
    "ecmaFeatures": {
        "modules": true,
        "classes": true,
        "jsx": true
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jasmine": true
    },
    "rules": {
        "strict": 1,
        "no-console": 2,
        "no-debugger": 2,
        "no-dupe-keys": 2,
        "no-unreachable": 1,
        "valid-jsdoc": 2,
        "guard-for-in": 2,
        "no-unused-vars": 1,
        "quotes": [2, "double"],
        "no-path-concat": 0,
        "no-shadow": 1,
        "no-use-before-define": 0
    },
    "plugins": [
        "eslint-plugin-react"
    ]
}
